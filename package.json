{
  "name": "ips-proxy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate && npx nodemon ./bin/www",
    "migrate": "npx sequelize-cli db:migrate",
    "revert": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo:all",
    "start": "node ./bin/www",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "~1.9.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.21",
    "sequelize-cli": "^6.6.0"
  }
}
